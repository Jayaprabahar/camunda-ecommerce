  <bpmn:process id="order-delivery" name="eCommerce" isExecutable="true">
    <bpmn:startEvent id="StartEvent_1" name="Checkout from Website">
      <bpmn:outgoing>Flow_Start</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:sequenceFlow id="Flow_Start" sourceRef="StartEvent_1" targetRef="Activity_ConfirmOrder" />
    <bpmn:sequenceFlow id="Flow_InformSeller" sourceRef="Activity_ConfirmOrder" targetRef="Activity_InformSeller" />
    <bpmn:sequenceFlow id="Flow_Shipment" sourceRef="Activity_InformSeller" targetRef="Activity_MakeShipment" />
    <bpmn:sequenceFlow id="Flow_CustomerReceiveItem" sourceRef="Activity_MakeShipment" targetRef="Activity_CustomerReceivedItem" />
    <bpmn:sequenceFlow id="Flow_CheckReflectionPeriod" sourceRef="Activity_CustomerReceivedItem" targetRef="Gateway_ProductReturnReflectionPeriod" />
    <bpmn:exclusiveGateway id="Gateway_ProductReturnReflectionPeriod" name="Product returned within Reflection Period">
      <bpmn:incoming>Flow_CheckReflectionPeriod</bpmn:incoming>
      <bpmn:outgoing>Flow_ProcessPayment</bpmn:outgoing>
      <bpmn:outgoing>Flow_1b5ma1z</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:exclusiveGateway id="Gateway_ProductReturnDefective" name="Is product returned in Good Condition">
      <bpmn:incoming>Flow_0y3rt4w</bpmn:incoming>
      <bpmn:outgoing>Flow_0l8hhld</bpmn:outgoing>
      <bpmn:outgoing>Flow_18jruqn</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:sequenceFlow id="Flow_ProcessPayment" name="No" sourceRef="Gateway_ProductReturnReflectionPeriod" targetRef="Activity_ProcessPayment">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">=(isPurchaseDate - currentDate) &gt; 30</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_0l8hhld" name="No" sourceRef="Gateway_ProductReturnDefective" targetRef="Activity_ReverseReturn">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">=isProductInGoodCondition = false</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_18jruqn" name="Yes" sourceRef="Gateway_ProductReturnDefective" targetRef="Activity_ReversePayment">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">=isProductInGoodCondition</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:serviceTask id="Activity_InformSeller" name="Inform Seller">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="informSeller" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_InformSeller</bpmn:incoming>
      <bpmn:outgoing>Flow_Shipment</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="Activity_MakeShipment" name="Make a Shipment">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="makeShipment" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_Shipment</bpmn:incoming>
      <bpmn:outgoing>Flow_CustomerReceiveItem</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="Activity_ProcessPayment" name="Process Payment to Seller">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="paymentToSeller" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_ProcessPayment</bpmn:incoming>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="Activity_ReverseReturn" name="Reverse item Return to Customer">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="reverseReturn" />
        <zeebe:ioMapping>
          <zeebe:output source="=reverseReturn" target="emailType" />
        </zeebe:ioMapping>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_0l8hhld</bpmn:incoming>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="Activity_ReversePayment" name="Reverse Payment">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="reversePayment" />
        <zeebe:ioMapping>
          <zeebe:output source="=reversePayment" target="emailType" />
        </zeebe:ioMapping>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_18jruqn</bpmn:incoming>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="Activity_ProductReturned" name="Product Returned">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="returnProduct" />
        <zeebe:ioMapping>
          <zeebe:output source="=isProductInGoodCondition" target="isProductInGoodCondition" />
        </zeebe:ioMapping>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_1b5ma1z</bpmn:incoming>
      <bpmn:outgoing>Flow_0y3rt4w</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="Activity_ConfirmOrder" name="Confirm Item Availability">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="confirmOrder" retries="5" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_Start</bpmn:incoming>
      <bpmn:outgoing>Flow_InformSeller</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="Activity_CustomerReceivedItem" name="Customer receives the items">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="customerReceiveItem" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_CustomerReceiveItem</bpmn:incoming>
      <bpmn:outgoing>Flow_CheckReflectionPeriod</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:sequenceFlow id="Flow_1b5ma1z" name="Yes" sourceRef="Gateway_ProductReturnReflectionPeriod" targetRef="Activity_ProductReturned">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">=(isPurchaseDate - currentDate) &lt;= 30</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_0y3rt4w" sourceRef="Activity_ProductReturned" targetRef="Gateway_ProductReturnDefective" />
    <bpmn:serviceTask id="Activity_0q3goza" name="Email Stock Unavailability">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="emailNoStock" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_1wy8aku</bpmn:incoming>
      <bpmn:outgoing>Flow_0hh398g</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:boundaryEvent id="Event_1bd2kdb" attachedToRef="Activity_ConfirmOrder">
      <bpmn:outgoing>Flow_1wy8aku</bpmn:outgoing>
      <bpmn:errorEventDefinition id="ErrorEventDefinition_1s6xhcj" errorRef="Error_1y6jn1z" />
